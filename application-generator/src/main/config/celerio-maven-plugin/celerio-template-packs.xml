<?xml version="1.0" encoding="UTF-8"?>
<celerio xmlns="http://www.jaxio.com/schema/celerio" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.jaxio.com/schema/celerio ./celerio.xsd">
    <configuration>
        <celerioTemplateContext>
            <entityContextProperties>
                <entityContextProperty property="application" subPackage="application" />
            </entityContextProperties>
        </celerioTemplateContext>
        <packs>
            <pack name="pack-backend-application" path="templates/application" enable="true" />
        </packs>
    </configuration>
</celerio>